<h2>Welcome</h2>

<table>
    <thead>
        <tr>
            <td>Name</td>
            <td>Token</td>
            <td>Action</td>
        <tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{name}}</td>
            <td>{{token}}</td>
            <td>
                <form action="plus" method="post">
                    <input type="hidden" name="id" value="{{id}}">
                    <button type="submit" name="token" value="{{token}}">Plus1</button>
                </form>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>